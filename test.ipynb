   "source": [
    "from pyspark.sql import SparkSession\n",
    "from typedspark import create_schema\n",
    "\n",
    "\n",
    "spark = SparkSession.Builder().getOrCreate()\n",
    "\n",
    "df = spark.createDataFrame([(\"Alice\", 24), (\"Bob\", 25)], [\"first-name\", \"age\"])\n",
    "\n",
    "ds, schema = create_schema(df)"
   ]
